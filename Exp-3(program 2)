#include<iostream>

using namespace std;
int main(){
   float m1;
   cout<<"enter the marks 1:";
   cin>>m1;
   float m2;
   cout<<"enter the marks 2:";
   cin>>m2;
   float m3;
   cout<<"enter the marks 3:";
   cin>>m3;
   float m4;
   cout<<"enter the marks 4:";
   cin>>m4;
   float m5;
   cout<<"enter the marks 5:";
   cin>>m5;
   float avg=(m1+m2+m3+m4+m5)/5;
   cout<<"average="<<avg<<endl;
   if(avg>=90){
       cout<<"grade is O";
   }
   else if(avg>=80 && avg<90){
       cout<<"grade is A+";
   }
   else if(avg>=70 && avg<80){
       cout<<"grade is B+";
   }
   else if (avg>=60 && avg<70){
       cout<<"grade is C+";
   }
   else if (avg>=50 && avg<60){
       cout<<"grade is D";
   }
   else {
       cout<<"grade is F";
   }
   
    return 0;
}
//Output:
enter the marks 1:78
enter the marks 2:85
enter the marks 3:91
enter the marks 4:82
enter the marks 5:93
average=85.8
grade is A+
